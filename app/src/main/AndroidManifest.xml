<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.multitagtext"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.activites.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activites.ExampleActivity" />
        <activity android:name=".views.activites.Example2Activity" />
        <activity android:name=".views.activites.Example3Activity" />
        <activity android:name=".views.activites.Example4Activity" />
        <activity android:name=".views.activites.Example5Activity" />
    </application>

</manifest>